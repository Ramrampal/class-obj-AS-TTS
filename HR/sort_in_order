#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int n;
    cin>>n;
    vector<int>p(n);
    for(int i=0;i<n;i++){
        cin>>p[i];
    }
        sort(p.begin(),p.end());
     
     for (int i = 0; i < n; i++) {
        cout << p[i] << " ";  
     }
    return 0;
     
}
