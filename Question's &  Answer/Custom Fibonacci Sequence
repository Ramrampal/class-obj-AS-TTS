"Modified Fibonacci Generator"
"Recursive Fibonacci Sequence"
"Custom Fibonacci Series"
*/


#include<iostream>
#include<vector>
using namespace std;
 int feb( int a, int b, int n){
     if( n==1){
         return a;
     }
     if( n==2){
         return b;
     }
     return  feb( a,b,n-1) + feb(a,b,n-2);
 }
  vector<int>findfeb( int a,int b,int m){
      vector<int > result;
      for( int i=1;i<m; i++){
          int x = feb( a,b,i);
          result.push_back(x);
      }
      return result;
  }
      int main(){
       int a,b,n;
       cin>>a>>b>>n;
       vector<int>q= findfeb( a,b,n);
       for(auto k:q){
           cout<<k<<" ";
           }
           return 0;
   }

2 3 7 // find 7th tarm

2 3 5 8 13 21
   
