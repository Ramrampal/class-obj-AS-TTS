
#include <iostream>
#include<vector> 
#include<map>
using namespace std;
int main(){
    int n ;
    cin>>n;
    vector<int > v(n);
    for( int i=0;i<n;i++){
        cin>>v[i];
    }
    map<int , int> m;
    for(auto i:v){
        m[i]++;
    }
        for(auto i: m){
            cout<<i.first<<" "<<i.second<<endl;
        }
    
}

/*
6
1 2 1 3 4 1
1 3
2 1
3 1
4 1


=== Code Execution Successful ===
*/

