// overriding
#include<bits/stdc++.h>
using namespace std;
class A{
    public:
    virtual void show(){
         cout<<" parent class";
    }
};
class B: public A{
     public:
     void show(){
          cout<<" child class";
     }
};
int main(){
    B b1;
    A *p = &b1;
    p -> show();
    return 0;
}
// out= child class
//ğŸ‘ğŸ‘ğŸ‘


#include <iostream>
using namespace std;

 class A {
      public:
      
      void  R(){
            cout<< " hii";
       }
       
 };
  class  B : public A {
      public:
    void    R(){
             cout<< " hello";
        }
  };
int main()
{
 B b;
 A *a1;
 a1 = &b;
 a1->R();
    return 0;
}                          //    ğŸ’¡ğŸ’¡ğŸ’¡ğŸ’¡ output : hii
